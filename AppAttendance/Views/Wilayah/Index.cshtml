@model IEnumerable<AppAttendance.Models.Wilayah>

@{
    ViewData["Title"] = "Index";
}
<p>
    <a asp-action="Create">Wilayah Baru</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nama_Wilayah)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nama_Wilayah)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Kd_wilayah">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Kd_wilayah">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Kd_wilayah">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
